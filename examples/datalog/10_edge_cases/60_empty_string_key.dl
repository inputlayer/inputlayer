% Test: Empty string as key
% Expected: Empty string works as key
% Category: 10_edge_cases
% Priority: P2 - Edge cases

.kg create test_empty_key
.kg use test_empty_key

+data[("", 100), ("a", 200), ("b", 300)].

?- data(Key, Val).

% Filter for empty key
empty_key(Val) :- data("", Val).

?- empty_key(Val).

.kg use default
.kg drop test_empty_key
