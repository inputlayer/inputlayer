% Test: Symmetric relation pattern
% Expected: Symmetric closure works
% Category: 10_edge_cases
% Priority: P2 - Edge cases

.kg create test_symmetric
.kg use test_symmetric

+friend[(1, 2), (2, 3), (1, 4)].

% Make symmetric: if (A, B) then also (B, A)
+symmetric(A, B) :- friend(A, B).
+symmetric(A, B) :- friend(B, A).

?- symmetric(A, B).

.kg use default
.kg drop test_symmetric
