% Test: Many wildcards in pattern
% Expected: Multiple wildcards work correctly
% Category: 10_edge_cases
% Priority: P1 - Edge cases

.kg create test_wildcards
.kg use test_wildcards

+wide[(1, 2, 3, 4, 5, 6)].
+wide[(10, 20, 30, 40, 50, 60)].

% Keep only first and last
ends(First, Last) :- wide(First, _, _, _, _, Last).

?- ends(First, Last).

% Keep only middle two
middle(C, D) :- wide(_, _, C, D, _, _).

?- middle(C, D).

% Keep every other
alternate(A, C, E) :- wide(A, _, C, _, E, _).

?- alternate(A, C, E).

.kg use default
.kg drop test_wildcards
