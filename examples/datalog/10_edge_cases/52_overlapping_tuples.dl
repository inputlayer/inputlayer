+intervals[(1, 10, 20), (2, 15, 25), (3, 30, 40), (4, 18, 22)].

% Find overlapping pairs (start1 < end2 AND start2 < end1)
overlaps(Id1, Id2) :- intervals(Id1, S1, E1), intervals(Id2, S2, E2), Id1 < Id2, S1 < E2, S2 < E1.

?- overlaps(Id1, Id2).

