% Test: Negative number handling

.kg create test_negative_n15t26
.kg use test_negative_n15t26

+nums[(-5,), (0,), (5,)].

% Operations with negatives
doubled(X, Y) :- nums(X), Y = X * 2.
inverted(X, Y) :- nums(X), Y = 0 - X.

?- doubled(X, Y).
?- inverted(X, Y).

.kg use default
.kg drop test_negative_n15t26
