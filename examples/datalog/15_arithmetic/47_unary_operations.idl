// Test: Unary minus and double negative

.kg create test_unary_n15t47
.kg use test_unary_n15t47

+nums[(5,), (10,), (-3,)]

// Negate values
negated(X, Y) <- nums(X), Y = 0 - X

?negated(X, Y)

// Double negative via subtraction
double_neg(X, Y) <- nums(X), Y = 0 - (0 - X)

?double_neg(X, Y)

.kg use default
.kg drop test_unary_n15t47
