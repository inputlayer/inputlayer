// Test: Modulo operation semantics

.kg create test_mod_n15t50
.kg use test_mod_n15t50

+vals[(7,), (10,), (15,), (8,)]

// Modulo operation
mod_3(X, Y) <- vals(X), Y = X % 3

?mod_3(X, Y)

// Check divisibility
divisible_by_5(X) <- vals(X), X % 5 = 0

?divisible_by_5(X)

.kg use default
