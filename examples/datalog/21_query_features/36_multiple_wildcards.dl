% Test: Multiple wildcards in query
% Expected: Ignore multiple columns
% Category: 21_query_features
% Priority: P2 - Query features

.kg create test_multi_wild
.kg use test_multi_wild

+wide[(1, "a", 10, true, 1.5), (2, "b", 20, false, 2.5)].

% Query with multiple wildcards
ids_only(Id) :- wide(Id, _, _, _, _).

?- ids_only(Id).

.kg use default
.kg drop test_multi_wild
