// Test: Duplicate column in query output

.kg create test_dup_col_n21t11
.kg use test_dup_col_n21t11

+nums[(1, 10), (2, 20)]

// Same column twice
dup(X, X, Y) <- nums(X, Y)

?dup(A, B, C)

// Same value from different columns
same_val(X, Y, Y) <- nums(X, Y)

?same_val(A, B, C)

.kg use default
.kg drop test_dup_col_n21t11
