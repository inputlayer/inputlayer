% Test: Symmetric difference
% Expected: Items in A or B but not both
% Category: 22_set_operations
% Priority: P2 - Set operations

.kg create test_sym_diff
.kg use test_sym_diff

+set_a[(1,), (2,), (3,)].
+set_b[(2,), (3,), (4,)].

% In A but not B
only_a(X) :- set_a(X), !set_b(X).

% In B but not A
only_b(X) :- set_b(X), !set_a(X).

?- only_a(X).
?- only_b(X).

.kg use default
.kg drop test_sym_diff
