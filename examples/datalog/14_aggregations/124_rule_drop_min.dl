% Test: Rule drop with min aggregation
% Description: Tests dropping a min rule and redefining it

.kg create test_drop_min_n14t124
.kg use test_drop_min_n14t124

+reading[(1, 25), (1, 22), (1, 28), (2, 30), (2, 35)].

+min_temp(Sensor, min<Temp>) :- reading(Sensor, Temp).

?- min_temp(X, Y).

.rule drop min_temp

?- min_temp(X, Y).

+min_temp(Sensor, min<Temp>) :- reading(Sensor, Temp).

?- min_temp(X, Y).

.kg use default
.kg drop test_drop_min_n14t124
