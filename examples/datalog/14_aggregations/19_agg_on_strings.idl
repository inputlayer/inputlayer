// Test: Aggregation on non-numeric (strings)

.kg create test_agg_strings_n14t19
.kg use test_agg_strings_n14t19

+names[(1, "alice"), (1, "bob"), (2, "carol"), (2, "dave"), (2, "eve")]

// COUNT should work on any type
string_count(G, count<N>) <- names(G, N)

?string_count(G, C)

// MIN/MAX on strings (lexicographic)
first_name(G, min<N>) <- names(G, N)
last_name(G, max<N>) <- names(G, N)

?first_name(G, N)
?last_name(G, N)

.kg use default
.kg drop test_agg_strings_n14t19
