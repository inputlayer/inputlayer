// Test: Integer arithmetic operations

.kg create test_int_arith_n11t33
.kg use test_int_arith_n11t33

+nums[(100,), (7,)]

// Various operations with 100
ops(N, Add, Sub, Mul, Div, Mod) <- nums(N), N = 100,
    Add = N + 23,
    Sub = N - 23,
    Mul = N * 3,
    Div = N / 7,
    Mod = N % 7

?ops(N, A, S, M, D, R)

.kg use default
.kg drop test_int_arith_n11t33
