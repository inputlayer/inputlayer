// Test: Boolean logic in filters

.kg create test_bool_logic_n11t27
.kg use test_bool_logic_n11t27

+flags[(1, true), (2, false), (3, true), (4, false)]

// Filter true values
enabled(Id) <- flags(Id, true)

// Filter false values
disabled(Id) <- flags(Id, false)

?enabled(Id)
?disabled(Id)

.kg use default
.kg drop test_bool_logic_n11t27
