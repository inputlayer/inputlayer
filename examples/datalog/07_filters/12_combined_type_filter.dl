% Test: Filter on multiple types

.kg create test_combined_filter_n07t12
.kg use test_combined_filter_n07t12

+products[(1, "Electronics", 500), (2, "Electronics", 100), (3, "Clothing", 50), (4, "Electronics", 200)].

% Electronics over $150
expensive_electronics(Id, Price) :-
    products(Id, "Electronics", Price),
    Price > 150.

?- expensive_electronics(Id, Price).

.kg use default
.kg drop test_combined_filter_n07t12
