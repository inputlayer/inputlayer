% Test: Sequence detection pattern

.kg create test_seq_n29t18
.kg use test_seq_n29t18

+events[(1, "login", 100), (2, "click", 110), (3, "purchase", 120), (4, "logout", 200)].

% Find events that follow login
post_login(Id, Action, Time) :-
    events(_, "login", LoginTime),
    events(Id, Action, Time),
    Time > LoginTime,
    Action != "login".

?- post_login(Id, Action, Time).

.kg use default
.kg drop test_seq_n29t18
