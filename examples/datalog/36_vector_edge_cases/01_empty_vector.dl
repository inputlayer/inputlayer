% Test: Empty Vector Operations
% Category: 36_vector_edge_cases
% Description: Tests operations on empty vectors []

.kg create test_empty_vec
.kg use test_empty_vec

% Insert empty vectors
+empty_vec[(1, []), (2, [])].

% Query the empty vectors
?- empty_vec(Id, V).

% Try distance calculation on empty vectors
dist(Id1, Id2, D) :- empty_vec(Id1, V1), empty_vec(Id2, V2), Id1 < Id2, D = euclidean(V1, V2).
?- dist(Id1, Id2, D).

% Cleanup
.kg use default
.kg drop test_empty_vec
