// Test: Asymmetric join (one side larger)

.kg create test_asym_join_n06t12
.kg use test_asym_join_n06t12

// Large relation
+items[(1, "a"), (2, "b"), (3, "c"), (4, "d"), (5, "e"), (6, "f"), (7, "g")]

// Small relation
+selected[(2), (5)]

// Join (small to large)
matched(Id, Val) <- items(Id, Val), selected(Id)

?matched(Id, Val)

.kg use default
.kg drop test_asym_join_n06t12
