% Test: String starts with check
% Expected: Filter strings by prefix
% Category: 31_strings
% Priority: P2 - String functions

.kg create test_starts
.kg use test_starts

+words[("hello",), ("help",), ("world",), ("held",)].

% Find words starting with "hel"
starts_hel(W) :- words(W), STARTS_WITH(W, "hel").

?- starts_hel(W).

.kg use default
.kg drop test_starts
