// Test: Graph bipartite-like pattern

.kg create test_bipartite_n18t86
.kg use test_bipartite_n18t86

+nodes[(1, "typeA"), (2, "typeB"), (3, "typeA"), (4, "typeB")]
+edges[(1, 2), (2, 3), (3, 4)]

// Edges between same type
same_type_edge(A, B) <-
    edges(A, B),
    nodes(A, TypeA),
    nodes(B, TypeB),
    TypeA = TypeB

?same_type_edge(A, B)

.kg use default
