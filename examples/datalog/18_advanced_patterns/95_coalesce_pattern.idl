// Test: Coalesce/default value pattern

.kg create test_coalesce_n18t95
.kg use test_coalesce_n18t95

+primary[(1, "Alice"), (2, "Bob")]
+backup[(1, "A1"), (3, "C3")]

// Use primary if exists, otherwise backup
combined(Id, Name) <- primary(Id, Name)
combined(Id, Name) <- backup(Id, Name), !primary(Id, _)

?combined(Id, Name)

.kg use default
.kg drop test_coalesce_n18t95
