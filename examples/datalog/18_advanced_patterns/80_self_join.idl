// Test: Self-join pattern

.kg create test_self_join_n18t80
.kg use test_self_join_n18t80

+friends[("Alice", "Bob"), ("Bob", "Carol"), ("Carol", "Dave"), ("Alice", "Carol")]

// Friends of friends (self-join)
friends_of_friends(A, C) <-
    friends(A, B),
    friends(B, C),
    A != C

?friends_of_friends(A, C)

.kg use default
