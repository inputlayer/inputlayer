// Test: Replace pattern (delete then insert)

.kg create test_replace_n02t11
.kg use test_replace_n02t11

// Initial data
+config[(1, "value1"), (2, "value2")]

?config(Id, Val)

// Replace value for key 1
-config(1, "value1")
+config(1, "new_value1")

?config(Id, Val)

// Replace another
-config(2, "value2")
+config(2, "new_value2")

?config(Id, Val)

.kg use default
.kg drop test_replace_n02t11
