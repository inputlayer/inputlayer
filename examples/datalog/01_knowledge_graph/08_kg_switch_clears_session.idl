// Test: KG switch clears session

.kg create test_kg_a_n01t08
.kg use test_kg_a_n01t08

+data[(1,)]

// Session rule
temp(X, Y) <- data(X), Y = X * 2

?temp(X, Y)

// Switch to another KG
.kg create test_kg_b_n01t08
.kg use test_kg_b_n01t08

// Session rule should be gone
?temp(X, Y)

.kg use default
.kg drop test_kg_a_n01t08
.kg drop test_kg_b_n01t08
