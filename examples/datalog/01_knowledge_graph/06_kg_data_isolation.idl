// Test: Knowledge graph data isolation

.kg create test_iso_a_n01t06
.kg use test_iso_a_n01t06

// Add data to first KG
+data[(1, "a"), (2, "b")]
?data(Id, Val)

// Create and switch to second KG
.kg create test_iso_b_n01t06
.kg use test_iso_b_n01t06

// This should be empty (no data from first KG)
?data(Id, Val)

// Add different data to second KG
+data[(99, "z")]
?data(Id, Val)

// Switch back to first - data should be unchanged
.kg use test_iso_a_n01t06
?data(Id, Val)

.kg use default
.kg drop test_iso_a_n01t06
.kg drop test_iso_b_n01t06
