// Test: Rule with same name as a relation

.kg create test_name_conflict_n17t18
.kg use test_name_conflict_n17t18

// Base facts (relation)
+items[(1, 10), (2, 20), (3, 30)]

?items(Id, Val)

// Rule that filters items (uses same name to extend)
+items(Id, Val) <- items(Id, Val), Val > 15

?items(Id, Val)

.kg use default
.kg drop test_name_conflict_n17t18
