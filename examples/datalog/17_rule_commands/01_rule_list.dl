// Test: Rule List Command
// Category: 17_rule_commands
// Description: Tests .rule command to list all defined rules

.db create test_rule_list
.db use test_rule_list

// Insert base data
+edge[(1,2), (2,3)].

// Define multiple rules
+reverse(X, Y) :- edge(Y, X).
+paths(X, Y) :- edge(X, Y).
+paths(X, Z) :- paths(X, Y), edge(Y, Z).

// List all rules
.rule

// Cleanup
.db use default
.db drop test_rule_list
