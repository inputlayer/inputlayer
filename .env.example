# Domain name for TLS certificate (required for production)
DOMAIN=inputlayer.example.com

# Admin password (auto-generated if not set, printed to logs on first boot)
INPUTLAYER_ADMIN_PASSWORD=

# Bootstrap API key (auto-generated if not set, printed to logs on first boot)
INPUTLAYER_BOOTSTRAP_API_KEY=

# Log level: trace | debug | info | warn | error
INPUTLAYER_LOG_LEVEL=info
